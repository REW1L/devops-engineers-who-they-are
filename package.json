{
  "name": "devops-landing-page",
  "version": "1.0.0",
  "description": "A simple landing page for DevOps Engineers built with React and served by Express.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
